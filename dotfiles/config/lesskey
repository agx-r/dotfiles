#command
^N	   forw-line
^P	   back-line
n	   forw-line-force
e	   back-line-force
N	   forw-scroll
E	   back-scroll

R	   repaint-flush
r	   repaint
^R	   repaint
^L	   repaint
\eu	   undo-hilite
\eU	   clear-search
g	   goto-line
\kh	   goto-line
<	   goto-line
\e<	   goto-line
p	   percent
%	   percent
\e[	   left-scroll
\e]	   right-scroll
\e(	   left-scroll
\e)	   right-scroll
\kl	   left-scroll
\kr	   right-scroll
\e{	   no-scroll
\e}	   end-scroll
{	   forw-bracket {}
}	   back-bracket {}
(	   forw-bracket ()
)	   back-bracket ()
[	   forw-bracket []
]	   back-bracket []
\e^F	   forw-bracket
\e^B	   back-bracket
G	   goto-end
\e>	   goto-end
>	   goto-end
\ke	   goto-end
\eG	   goto-end-buffered
=	   status
^G	   status
:f	   status
/	   forw-search
?	   back-search
\e/	   forw-search *
\e?	   back-search *
n	   repeat-search
\en	   repeat-search-all
N	   reverse-search
\eN	   reverse-search-all
&	   filter
m	   set-mark
M	   set-mark-bottom
\em	   clear-mark
'	   goto-mark
^X^X	   goto-mark
:e	   examine
^X^V	   examine
:n	   next-file
:p	   prev-file
t	   next-tag
T	   prev-tag
:x	   index-file
:d	   remove-file
-	   toggle-option
:t	   toggle-option t
s	   toggle-option o

	    ## Use a long option name by starting the extra string with ONE dash; eg:
	    ##	 s toggle-option -log-file
_	   display-option
|	   pipe
v	   visual
!	   shell
+	   firstcmd
H	   help
h	   help
V	   version
0	   digit
1	   digit
2	   digit
3	   digit
4	   digit
5	   digit
6	   digit
7	   digit
8	   digit
9	   digit
q	   quit
Q	   quit
:q	   quit
:Q	   quit
ZZ	   quit

